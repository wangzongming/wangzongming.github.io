   body {
            margin: 0;
            font-family: "微软雅黑" sans-serif;
        }
        
        table {
            width: 100%;
            text-align: left;
        }
        
        table td {
            word-break: break-all;
            word-wrap: break-word;
            border: 1px solid #98b8d6;
            font: 500 14px Arial;
            vertical-align: top;
        }
        
        ul {
            margin: 0;
            padding: 0;
            list-style: none;
        }
        .ny_color_blue1{
            background-color:#9BC8F2
        }
        .ny_color_blue2{
            background-color:#9BC8F2
        }
        .ny_module {
          
        }
        
        .ny_module.full {
            position: absolute;
            left: 0;
            right: 0;
            top: 0;
            bottom: 0;
        }
        
        .ny_header {
            padding: 0px 10px;
            color: #002fac;
            font-size: 18px;
            line-height: 30px;
            height: 30px;
            background-image: url("../img/top.png");
            background-repeat: repeat-x;
            white-space: nowrap;
            text-overflow: ellipsis;
            overflow: hidden;
        }
        .ny_header::after{
            content: "";
            clear:both;
            display: table;
            overflow: hidden;
        }
        .ny_more {
            float:right;
            margin-right:10px;
            text-decoration: none;
            color: #002fac;
            font-size: 12px;
            display: block;
            cursor: pointer;
        }
        
        .ny_body {
            padding: 5px 10px;
        }
        
        .ny_list {}
        
        .ny_listItem {
            height: 28px;
            line-height: 28px;
            color: #487aa0;
            position: relative;
            padding-right: 5px;
            cursor: pointer;
        }
        
        .ny_listItem+.ny_listItem {
            border-top: 1px #aaa dashed;
        }
        
        .ny_itemCon {
            color: #487aa0;
            position: absolute;
            top: 0;
            bottom: 0;
            left: 0;
            right: 85px;
            white-space: nowrap;
            text-overflow: ellipsis;
            overflow: hidden;
        }
        
        .ny_itemTag {
            text-align: right;
            float: right;
            width: 80px;
            font-size: 12px;
            color: #999;
			white-space: nowrap;
            text-overflow: ellipsis;
            overflow: hidden;
        }
        
        .ny_itemLabel {
            display: inline-block;
            font-size: 13px;
            color: #ffaa24;
            margin-right: 5px;
        }
        
        .ny_itemCon.icon {
            padding-left: 24px;
            background-image: url(../img/右.png);
            background-repeat: no-repeat;
            background-size: 20px 20px;
            background-position: left center;
        }
        
        .ny_bsItem {
            background-color:#67B0E1;
            height: 80px;
            color: #fff;
            border: none;
            cursor: pointer;
            text-align: center;
            vertical-align: middle;
            font-size: 14px;
            line-height: 1.5;
        }
        
        .ny_tab {
            width: 500px;
            margin: 0 auto;
            height: 40px;
             margin-top:10px;
        }
        
        .ny_tabBox {
            height: 39px;
            padding:0 10px;
        }
        
        .ny_tabBox::after {
            content: "";
            display: table;
            clear: both;
        }
        .ny_bsTable{
            table-layout: fixed;
            border-collapse:separate;
        }
        .ny_tabItem {
            text-align: center;            
            float: left;
            height: 39px;
            line-height: 39px;
            color: #487aa0;
            background-color: #deeefe;
            cursor: pointer;
            white-space: nowrap;
            text-overflow: ellipsis;
            overflow: hidden;
        }
        .ny_tabItem+.ny_tabItem{
            margin-left: 5px;
        }
        .ny_tabItem.cur{
           background-image: url("../img/btn.png")
        }
        
        .ny_indexItem2 {
            cursor: pointer;
            vertical-align: bottom;
            text-align: right;
            background-repeat: no-repeat;
            background-position: 10px 10px;
            height: 110px;
            padding: 30px 10px 10px 30px;
        }
        .ny_indexItem {
                border: 1px solid #98b8d6;
            cursor: pointer;
    vertical-align: bottom;
    text-align: right;
    background-repeat: no-repeat;
    background-position: 10px 10px;
    height: 63px;
    padding: 10px 10px 10px 30px;
        }
        
        .ny_indexItemText {
            color: #fff;
            font-size: 28px;
            line-height: 1;
            white-space: nowrap;
        }
        
        .ny_indexItemText b {
            font-size: 14px;
            font-weight: normal;
        }
        .ny_task_l1{
            margin-top:20px;
            white-space: nowrap;
            text-overflow: ellipsis;
            overflow: hidden;
            height: 100px;
            line-height: 100px;
            font-size:32px;
            color: #002fac;
            font-weight: bold;
        }
        .ny_task_l1 span{
font-size: 70px;color: #8cc63e;
        }
         .ny_task_l2{
             padding-right:20px;
             text-align:right;
             font-size:20px;
             line-height:30px;
             color: #002fac;
        }
        .ny_task_l2 a{
text-decoration: none;
        }
        .pagination {
  position: absolute;
  z-index: 20;
  bottom: 10px;
  width: 100%;
  text-align: center;
}
.swiper-container{
    width: 500px;
    height:194px;
}
.swiper-pagination-switch {
  display: inline-block;
  width: 8px;
  height: 8px;
  border-radius: 8px;
  margin: 0 5px;
  opacity: 0.8;
  border: 1px solid #fff;
  cursor: pointer;
}
.swiper-active-switch {
  background: #fff;
}
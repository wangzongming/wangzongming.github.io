html { font-family: "Microsoft YaHei","Î¢ÈíÑÅºÚ"; }
html, body { height: 100%; }
body { max-width: 1920px; margin: 0 auto; min-width: 1200px; background-color:#f7f7f7;}
a { color: inherit; text-decoration: none; }
input, textarea {    border: 0 none;padding:0;font-family: "Microsoft YaHei","Î¢ÈíÑÅºÚ"; }

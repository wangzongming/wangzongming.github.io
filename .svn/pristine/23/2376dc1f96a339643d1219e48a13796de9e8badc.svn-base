.mid { text-align: center; }
    .mid:before { content: ""; width: .1em; height: 100%; display: inline-block; vertical-align: middle; }
    .mid > .mid_con { display: inline-block; vertical-align: middle; }

.clear { *zoom: 1; }
    .clear:after { content: ""; display: block; visibility: hidden; clear: both; line-height: 0; height: 0; }
.f_l { float: left; }
.f_r { float: right; }

.swiper-mybuttonBox { padding: 20px 0; }
    .swiper-mybuttonBox .swiper-mybutton-prev { user-select: none; display: inline-block; width: 30px; height: 30px; line-height: 27px; font-size: 25px; border-radius: 4px; text-align: center; color: #fff; background-color: #fc1330; cursor: pointer; }
    .swiper-mybuttonBox .swiper-mybutton-next { margin-left: 15px; user-select: none; display: inline-block; width: 30px; height: 30px; line-height: 27px; font-size: 25px; border-radius: 4px; text-align: center; color: #fff; background-color: #18171c; cursor: pointer; }
.mark { opacity: 0; position: absolute; left: 0; right: 0; top: 0; bottom: 0; background-color: rgba(47,208,181,.6); transition: opacity .3s; cursor: pointer; }
    .mark:hover { opacity: 1; }


.page { min-height: 100%; box-sizing: border-box; padding-bottom: 270px; position: relative; }



/*==================== Common Start ====================*/


/* Base */

body,
html {
	min-width: 1200px;
	font-family: Arial, Verdana, "微软雅黑", "宋体";
	color: #333;
	font-size: 16px;
	line-height: 1.8;
	-webkit-text-size-adjust: none;
}

.width_all {
	width: 1200px;
	margin: 0 auto;
}

ul,
h2,
h3,
dl,
dt,
dd,
p {
	padding: 0;
	margin: 0;
}

li {
	list-style: none;
}


/* Font */

.font_12 {
	font-size: 13px;
}

.font_13 {
	font-size: 13px;
}

.font_14 {
	font-size: 14px;
}

.font_16 {
	font-size: 16px;
}

.font_18 {
	font-size: 18px;
}

.font_20 {
	font-size: 20px;
}

.font_22 {
	font-size: 22px;
}

.font_23 {
	font-size: 23px;
}

.font_24 {
	font-size: 24px;
}

.font_26 {
	font-size: 26px;
}

.font_28 {
	font-size: 28px;
}

.font_30 {
	font-size: 30px;
}

.font_32 {
	font-size: 32px;
}

.font_38 {
	font-size: 38px;
}

.font_40 {
	font-size: 40px;
}

.font_48 {
	font-size: 48px;
}

.font_52 {
	font-size: 52px;
}

.font_B {
	font-weight: bold;
}


/* Float */

.float_r,
.float_l {
	display: inline;
	zoom: 1;
}

.float_r {
	float: right;
}

.float_l {
	float: left;
}

.clear::after {
	content: "";
	display: block;
	height: 0;
	_height: 0;
	overflow: hidden;
	clear: both;
}


/*====================Common End====================*/


/*====================Main Start====================*/


/* Header */

.header {
	position: relative;
	z-index: 9;
}

.top {

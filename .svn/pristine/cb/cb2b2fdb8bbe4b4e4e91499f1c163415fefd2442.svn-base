html, body {  width: 100%;overflow-x:hidden; }
html { font-family: "微软雅黑";-ms-text-size-adjust: 100%; -webkit-text-size-adjust: 100%; font-size: 62.5%; height:100%;
}
body {position:relative; margin: 0; font-size: .75rem; line-height: 1.5; color: #2e2e2e; background-color: #f2f2f2; box-sizing:border-box; -webkit-overflow-scrolling: touch;   }
p,ul,h3,h4,ol{ margin:0; padding:0;}
h3,h4{ font-weight:normal;}
a{ text-decoration:none; color:inherit;}
li{ list-style:none;}
img{ width:100%;}
.fr{float:right;}
.fl{float:left;}
.clear { *zoom: 1; }
.clear:after { content: ""; display: block; visibility: hidden; clear: both; line-height: 0; height: 0; }


.footer_nav,.nav_top,.nav_bottom,.brand,header,.product_ul,.area_ul,.train_ul,.method_ul li,.apply_ul,.apply_ul2,.news_list,.after_sale_ul,header,.btn,.nav_code div{display: -webkit-box; display: -ms-flexbox; display: -webkit-flex; display: flex;}
.footer_nav li,.nav_top li,.nav_bottom li,.product_ul li,.btn{flex:1; -webkit-flex:1;}
.nav_code div{align-items:center;-webkit-align-items:center;justify-content:center;-webkit-justify-content:center;}
.brand,.area_ul,.train_ul,.apply_ul,.apply_ul2,.after_sale_ul{ flex-flow:row wrap;-webkit-flex-flow:row wrap;}

/*.main{ background:#fff; position:relative;height:100%;overflow-x:hidden;}*/
.p12{padding:0 1.2rem;}
.grey{ background:#efefef; }
header{ position:fixed; top:0; left:0; width:100%; padding:0 1rem; background:#010101; box-sizing:border-box;z-index:999;
}
/*.btn{ position:absolute; right:1rem;}*/
.btn{ justify-content:flex-end;-webkit-justify-content:flex-end;}
.btn span{ display:block;}
.logo{ width:12rem; height:1.7rem; padding:.9rem 0;}
.search{ width:3rem;height:3.3rem; background:url(../img/search.png) no-repeat center center; background-size:1.5rem 1.5rem; }
.menu{ padding:.9rem 0 ; width:2.5rem;height:1.5rem; background:url(../img/menu.png) no-repeat center center;background-size:1.4rem 1.4rem;}
.search.on{ background:url(../img/search_on.png) no-repeat #fff center center; background-size:1.5rem 1.5rem; }
.menu.on{ background:url(../img/close.png) no-repeat center center;background-size:1.4rem 1.4rem;}
.search_input{ padding:.25rem; position:fixed; top:3.5rem;left:0; z-index:998;right: 0; background:#fff;}
.search_input input{ border:1px solid #2e2e2e; border-radius:.2rem; background:#FFF; font-size:1rem; padding-right:1.5rem;
width:100%; line-height:1; outline:none; box-sizing:border-box;}
.search_btn{ position:absolute; right:.75rem; top:.7rem; width:1rem; height:1rem;}
.search_btn img{ width:100%;}
.content{padding-top:3.5rem;height: 83%;  position:relative; background: #fff; padding-bottom:5rem;}
.swiper-container { width: 100%;}
.swiper-pagination-bullet {background: #e6e6e6; opacity: 1;}
.swiper-pagination-bullet-active{ background:#e55b27;}
.nav_top{ padding-left:2rem; background:#f7f7f7;}
.nav_top li{margin-right:1.7rem; color:#393939; font-size:.7rem; text-align:center; padding:.85rem 0 .6rem 0;}
.nav_top li img{  width:2.5rem; margin-bottom:.6rem;}
.title{ font-size:1.2rem; color:#191919; position:relative; padding-left:1.2rem; padding-top:2rem; padding-bottom:2.25rem;}
.title img{ height:.9rem; width:2rem; margin-left:.3rem;}
.title::after{ content:"";width:.95rem;height:1px; background:#191919; position:absolute;bottom:1.2rem; left:1.25rem;}
.article{ font-size:.75rem; line-height:1.45rem; margin:0 1.2rem; color:#282828;}
.border{ padding-bottom:2rem;border-bottom:1px solid #cecece;}
.quality_con{ background:#efefef; padding:1.85rem 2.5rem; position:relative; margin-top:1.5rem;}
.eng{ font-size:.4rem; text-align:center; padding-top:.5rem;}
.ch{ font-size:.8rem; text-align:center;color:#000;}
.swiper-button-next{ background:url(../img/right.png) no-repeat; }
.swiper-button-prev{ background:url(../img/left.png) no-repeat;} 
.swiper-button-next,.swiper-button-prev{background-size:.8rem 1.5rem; width:.8rem; height:1.5rem;}
.example{padding:2rem 2.5rem; position:relative;}
.example .swiper-slide{ background:#efefef; text-align:center;padding:1.6rem 0;}
.example .swiper-slide img{width:2.6rem;  margin-bottom:.75rem;}
.example  .swiper-button-next,.swiper-button-prev{top:58%;}
.company_con{ background:#f7f7f7; padding-top:2rem;}
.company{font-size:1.2rem; color:#191919; position:relative; text-align:center; margin-bottom:2.1rem;}
.company::before,.company::after{content:"";width:.95rem;height:1px; background:#191919; position:absolute;top:.85rem;}
.company::before{left:36%;}
.company::after{right:36%;}
.brand li{ width:33%;border-right:1px solid #afafaf; text-align:center; margin-bottom:2.4rem;}
.brand li img{  width:5.375rem;}
.brand li:nth-child(3n){border:none;}
.brand li:last-child{border:none;}
video{ width:100%;height:17rem;}
.nav,.nav_code{ position:fixed; top:0; left:0; right:0; bottom:0; z-index:1000;background:rgba(0,0,0,0.1); display:none;overflow:hidden;}
.nav_code div{ width:100%; height:100%;}
.nav_bottom{  padding:0 1.1rem; padding-bottom:1.5rem; margin-top:1.85rem;}
.nav_bottom li{ margin-right:2.2rem; text-align:center; }
.nav_bottom li:last-child{ margin-right:0;}
.nav_bottom li img{ width:3.25rem;}
.nav_swift{ position:absolute; right:-19rem; top:3.5rem; background:#fff; z-index:999; width:18rem; padding-top:1.5rem;bottom: 0;overflow-y:scroll;}
.nav_swift li{ padding-left:1.75rem; padding-right:2rem; background:url(../img/right.png) no-repeat right 1.47rem center;
background-size:.53rem .96rem; font-size:.8rem; color:#272727; margin-bottom:2rem;}
.nav_swift li.on{color:#df2e00;}
footer{position:fixed; bottom:0; left:0;width:100%; z-index:999;}
.footer_nav{background:#010101; padding:.25rem 0; font-size:.6rem; color:#fcfcfc;}
.footer_nav li{ text-align:center; padding-bottom:.25rem;}
.footer_nav li+li{border-left:1px solid #5b5b5b;}
.footer_nav li img{ width:1.5rem; height:1.8rem; display:block; margin:.25rem auto;}
.footer_nav li:nth-child(3) img{ width:1.8rem; }

/*产品中心*/
.content.grey{ background:#e7e7e7;}
.product{ padding:0 .8rem;}
.product_title{ font-size:1rem; color:#0a0a0a; padding-top:1.1rem; padding-bottom:.9rem;}
.product_ul li{  background:#f2f2f2;}
.product_ul li+li{margin-left:.5rem;}
.product_content{ padding-top:.6rem; padding-left:.6rem; padding-bottom:.75rem; font-size:.7rem; line-height:.85rem;}
.more{ text-align:right; padding-right:1.5rem; background:url(../img/down.png) no-repeat right .1rem center;
background-size:.95rem .95rem; padding-top:1.1rem; padding-bottom:1rem; border-bottom:1px solid #ababab;}

/*服务支持*/
.consult,.after_sale{ background:#f7f7f7; padding:0 1.1rem; padding-bottom:1.75rem;}
.consult_title{font-size:1.2rem; color:#191919; padding-top:1.75rem; padding-bottom:1.25rem;}
.area{ background:#efefef; padding:0 1.1rem; margin-top:1.75rem;}
.area_ul{padding-top:1.75rem; padding-bottom:1.5rem;}
.area_li{ border:1px solid #686868; margin-right:.25rem; margin-bottom:.25rem; width:48.6%; padding:0 .5rem; box-sizing:border-box; padding-bottom:.4rem;}
.area_li:nth-child(2n){margin-right:0;}
.area_title{color:#0e0e0e; font-size:1.2rem;position:relative; padding-top:1rem; padding-bottom:1.5rem;}
.area_title::after{ content:"";width:1.25rem;height:1px; background:#787072; position:absolute;bottom:.75rem; left:.5rem;}
.area_content li{ margin-bottom:.45rem;}
.nav_bottom.support_bottom{padding-bottom:1.5rem;}
.nav_bottom.support_bottom li{ margin-right:1.25rem;}
.nav_bottom.support_bottom li:last-child{ margin-right:0;}
.train{ padding:0 1.1rem; padding-top:1.1rem;}
.train_ul{ padding-top:1.5rem;}
.train_li{width:49%; margin-bottom:1.75rem; margin-right:2%;}
.train_li:nth-child(2n){ margin-right:0;}
.train_li h3{font-size:1.2rem; color:#191919; margin-bottom:.7rem;}
.train_li img{ width:1.7rem;vertical-align: middle; margin-right:.5rem;}
.train_content li{ margin-bottom:.5rem;color:#646464;}
.nav_bottom.support{ margin-top:1.25rem; padding-bottom:1.75rem;}
.after_sale_h3{font-size:1.2rem; color:#191919; padding-top:1.5rem; padding-bottom:1.4rem; text-align:center;}
.after_sale_li { margin-bottom:2.75rem;}
.train_li.after img{ width:1.7rem;}
.after_sale_ul .train_li:nth-child(2){ padding-top:2.5rem;}

/*关于我们*/
.about{ padding-bottom:.75rem;}
.about .title,.core .title{padding-top:0;}
.about .article{margin-bottom:1rem;}
.about_title{font-size:1.2rem; color:#191919; position:relative; padding-left:1.2rem; padding-top:1.25rem;}
.about_title span{color:#d4591e;font-size:1rem; position:absolute; top:1.4rem; right:2.5rem;}
.core_con{ background:#e7e7e7; margin-top:.5rem;}
.core{ padding:0 1.2rem; padding-top:2rem; padding-bottom:2.2rem;}
.core li{ border:1px solid #2e2e2e;margin-bottom:1.1rem; padding-bottom:1rem;}
.core_content{padding:0 .8rem; line-height:1.5rem;color:#141414; font-size:.8rem;}
.core .about_title{ padding-left:.8rem; padding-top:1rem;}
.core .title{ padding-left:.8rem; }
.core .title::after{ left:.8rem; width:1.5rem;}

/*解决方案*/
.plan .article{ border-bottom:1px solid #b7b7b7; padding-bottom:2rem;}
.method_ul{ margin-top:1.2rem; margin-bottom:1.25rem;}
.method_ul li{color:#27a9e0; font-size:.75rem;justify-content:space-between;-webkit-justify-content:space-between;
margin-bottom:.6rem;}
.method:last-child{ padding-bottom:7rem;}

/*联系我们*/
.contact{ background:#e7e7e7; color:#2b2b2b; font-size:.75rem; padding-bottom:16rem;}
.ch_contact,.eng_contact{ width:25%; text-align:right;padding-left:2.5rem;}
.ch_contact{ font-size:1.2rem; padding-top:1.2rem; }
.eng_contact{ padding-bottom:.7rem; border-bottom:1px solid #2b2b2b;}
.contact_ul{ padding:0 2.6rem; padding-top:1.5rem;}
.contact_ul li{ padding-bottom:1.25rem;}
.name{ line-height:1.5rem;}
.detail{ width:100%; height:1.7rem; border-radius:.75rem; background:#fff; padding-left:.8rem;line-height:1.55rem;
color:#6b6b6b; font-size:.75rem; box-sizing:border-box; box-shadow: 3px 3px 8px #888888;}
.detail img{ vertical-align:middle; width:1.1rem; height:1rem; margin-right:1.25rem; }

/*应用领域*/
.article.apply{ padding-top:1.5rem; padding-bottom:2rem;}
.apply_title{ background:#0d6db9; width:6.25rem; height:1.6rem; line-height:1.6rem; font-size:1rem; color:#fff; text-align:center; position:relative; }
.apply_title::after{content:"";width:1.6rem;height:1.6rem; background:#0d6db9; position:absolute;top:0; right:-.8rem;
border-radius:50%;}
.apply_ul{ padding:0 1.2rem; padding-top:1.75rem;}
.apply_ul li{ width:31.7%; margin-right:.5rem; text-align:center; margin-bottom:.75rem;}
.apply_ul li img{ margin-bottom:.4rem;}
.apply_ul li:nth-child(3n){ margin-right:0;}
.apply_ul2{ background:#e7e7e7; padding:0 1.2rem; padding-bottom:1rem;}
.apply_ul2 li{ width:48.8%; margin-right:.5rem; margin-bottom:.3rem;}
.apply_ul2 li:nth-child(2n){ margin-right:0;}
.apply_title2{background:#e7e7e7; padding:1.75rem 0;font-size:1rem; text-align:center; position:relative; }
.apply_title2::after{ content:"";width:1.2rem;height:1px; background:#191919; position:absolute;bottom:1.2rem; left:47%;}
.content section:last-child{ /*padding-bottom:4rem;*/}
.more.apply{ padding-right:2.3rem; background:url(../img/down.png) no-repeat right 1.1rem center;
background-size:1rem 1rem; border:none; }

/*新闻中心*/
.news { background:#e7e7e7; padding:0 .9rem;}
.news h3{font-size:1.1rem; padding-top:1.4rem; padding-bottom:1rem; border-bottom:1px solid #8a8a8a; padding-left:.2rem;
margin-bottom:.8rem;}
.year{ color:#4c4c4c; font-size:1rem;}
.news_list_ul{ padding-top:.55rem;}
.news_list{ padding:.25rem; background:#fff; position:relative; margin-bottom:.5rem;}
.news_list img{ width:12.5rem; height:6.75rem; margin-right:.75rem;}
.news_content{ font-size:1rem; line-height:1.5rem; padding-top:.55rem;}
.time{ font-size:.8rem; color:#7d7d7d; text-align:right; position:absolute; right:.55rem; bottom:.55rem;}
.more.left{ position:relative; padding-top:1.5rem; padding-bottom:1.5rem;}
.more.left .year{ position:absolute; left:.2rem;top:1.5rem;}

/*新闻详情*/
.news_detail{ padding:0 1.4rem; font-size:.75rem; padding-top:1.5rem; background:#fff; padding-bottom:5rem;}
.news_detail h3{ font-size:1.3rem; line-height:1.7rem; margin-bottom:.55rem;}
.news_detail h4{ padding-bottom:.8rem; border-bottom:1px solid #9c9c9c; margin-bottom:1.4rem;}
.blue{color:#50a1d9; margin-left:.6rem;}
.news_detail_content{ line-height:1.2rem; margin-bottom:1rem;}
.news_detail img{ margin-bottom:.9rem;}
.line { text-align:center;  margin-bottom:1.5rem;}
.line  span{ line-height:.9rem; border-bottom:1px solid #9c9c9c;}
.history { padding-top:.5rem;}
.history li{ margin-bottom:.75rem; }

/*产品详情*/
.product_detail{ padding:0 1rem; font-size:.75rem; padding-top:1.2rem;padding-bottom:5rem; background:#fff;}
.product_detail_title{color:#50a1d9; font-size:1.1rem; margin-bottom:.75rem;}
.product_detail_content{ line-height:1.2rem; margin-bottom:1.2rem;}
.choose{  height:1.35rem;line-height:1.35rem; background:#dadada url(../img/choose.jpg) no-repeat right .1rem center; background-size:1rem; margin-bottom:1.1rem;
padding-left:.5rem; box-sizing:border-box; border:1px solid #b4b4b4;}
.apply_ul.p0{ padding:0;}
.tab{ width:100%;border-collapse:collapse; }
.tab tr th{ height:1.2rem;  background: -webkit-linear-gradient(#e5e5e5, #bfbdbe); /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(#e5e5e5,#bfbdbe); /* Opera 11.1 - 12.0 */
  background: -moz-linear-gradient(#e5e5e5,#bfbdbe); /* Firefox 3.6 - 15 */
  background: linear-gradient(#e5e5e5,#bfbdbe); /* 标准的语法 */
 text-align:left; border:1px solid #a09e9f;}
 .tab tr td{ width:49.5%; border-right:1px solid #a09e9f; border-bottom:1px solid #a09e9f;}
 .tab tr td:nth-child(odd){ background:#e5e5e5; border-left:1px solid #a09e9f; }
 .tab span{ padding:0 .5rem;}

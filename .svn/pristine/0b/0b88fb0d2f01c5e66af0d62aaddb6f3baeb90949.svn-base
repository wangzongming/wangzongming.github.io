<script language="javascript">
    var mySwiper = new Swiper('.product-swiper1', {
        slidesPerView: 'auto',
        prevButton: '.product-swiper1 .swiper-button-prev',
        nextButton: '.product-swiper1 .swiper-button-next',
    })
    $(".product-swiper1").on("mouseenter", ".swiper-slide:not(.cur)", function () {
        $(this).find(".pro_img").stop(true, true).slideDown();
    })
    $(".product-swiper1").on("mouseleave", ".swiper-slide:not(.cur)", function () {
        $(this).find(".pro_img").stop(true, true).slideUp();
    })
    var mySwiper = new Swiper('.product-swiper2', {

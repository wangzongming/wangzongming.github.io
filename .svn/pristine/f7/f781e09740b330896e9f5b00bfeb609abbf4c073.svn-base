.cust-area6{
    background-color: #000000;
    min-height: 12.07rem;
    display: block;
    position: relative;
    
    .cust-particles{
        position: absolute;
          top: 0px;
          right: 0px;
          left: 0px;
          bottom: 0px;
        z-index: 1;
    }
    
    >.container-fluid{
        position: absolute;
        z-index: 9;
        top: 0px;
        bottom: 0px;
        left: 0px;
        right: 0px;
    }
    
    .cust-h1{
        display: block;
        text-align: center;
        margin-top: 1.22rem;
        line-height: 0.44rem;
        font-size: 0.44rem;
        line-height: 0.44rem;
        color: #010101;
        margin-bottom: 0.88rem;
    }
    
    .cust-videa-area{
        display: block;
        width: 13.05rem;
        max-width: 80%;
        height: 7.44rem;
        margin-left: auto;
        margin-right: auto;
        position: relative;
        margin-bottom: 0.76rem;
        
        img{
            display: block;
            /*
            width: 13.05rem;
            height: 7.44rem;
            */
            width: 100%;
            height: auto;
        }
        
        a{
            position: absolute;
            display: block;
            top: 50%;
            left: 50%;
            margin-left: -0.52rem;
            margin-top: -0.05rem;
            background-color: #FFFFFF;
            border: 1px solid @color-orange;
            border-radius: 50%;
            width: 1.04rem;
            height: 1.04rem;
            font-size: 0px;

            -webkit-transition: all .1s ease-out;
            transition: all .1s ease-out;
            
            &:hover,&:focus{
                 text-decoration: none;
            }
            
            span{
                display: block;
                text-align: center;
                line-height: 1.04rem;
                color: @color-orange;
                font-size: 1.34rem;
                
            }
        }
        
        & a:hover{
                -webkit-transform: scale(1.2);
                transform: scale(1.2);
        }
        
    }
    
    
    .cust-videofile-btn{
        margin-bottom: 0.92rem;
        font-size: 0.19rem;
        line-height: 0.19rem;
        padding-top: 0.12rem;
        padding-bottom: 0.12rem;
        padding-left: 0.38rem;
        padding-right: 0.38rem;
        display: block;
        margin-left: auto;
        margin-right: auto;
        background-color: @color-orange;
        color: #FFFFFF;
        border-radius: 0px;
        border:0px;
    }
}








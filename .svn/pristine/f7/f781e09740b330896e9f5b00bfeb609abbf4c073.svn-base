@carousel-pointer-width:30px;
.cust-carousel{
    position: relative;
    
   .cust-carousel-content{
       position: absolute;
       top:  0px;
       left: 0px; 
       width: 100%;
       padding-top: 2.5rem;  
       padding-left: 15px;
       padding-right:15px ;
       margin-left: 15px;
       margin-right: 15px;
       z-index: 99;
       
       >img{
            position: absolute; 
            top: 0.60rem;
            left: 15px;
            height: 0.60rem;
            width: auto;
       }
       
       .cust-carousel-h1{
           font-size: 0.54rem;
           line-height: 0.54rem;
           color: @color-orange; 
           margin-bottom: 0px;
           display: block;
       }
       .cust-carousel-h3{
           font-size: 0.25rem;
           line-height: 0.25rem;
           color: #F6F6F6;
           margin-bottom: 0px;
           display: block;
       }
       .cust-carousel-h4{
           font-size: 0.24rem;
           line-height: 0.24rem;
           color: @color-orange;
           margin-bottom: 0px;
           display: block;
       }
       .cust-carousel-h5{
           display: inline-block;
           font-size: 0.26rem;
           line-height: 0.36rem;
           color: #FFFFFF;
           margin-bottom: 0px;
           font-weight: bold;
           padding-left: 0.5rem;
           padding-right: 0.5rem; 
           border: 1px solid #FFFFFF;
           height: 0.36rem;
           float: left;
           margin-left: 0.10rem;
           border-radius: 0.08rem;
           letter-spacing: 0.1em;
       }
       .cust-carousel-btn{
            height: 0.36rem;
            font-size: 0.26rem;
            line-height: 0.26rem;
            padding-top: 0px;
            padding-left: 0.2rem;
            padding-right: 0.2rem; 
            padding-bottom: 0px;
            background-color: #FFFFFF;
            border-radius: 0px;
            border:0px;
            color: #000000;
            display: inline-block;
            float: left;
            border-radius: 0.08rem;
            /*margin-left: @carousel-pointer-width;*/
       }
       .first{
           margin-bottom: 0.19rem;
           /*margin-left: @carousel-pointer-width;*/
       }
       .second{
           margin-bottom: 0.43rem;                   
           /*margin-left: @carousel-pointer-width;*/
       }
       .third{
           margin-bottom: 0.16rem;
           /*margin-left: @carousel-pointer-width;*/
       }
       .forth{
           margin-bottom: 0.6rem;
           /*margin-left: @carousel-pointer-width;*/
       }
       .fifth{
           margin-bottom: 0.24rem;
       }
       
       &.blue{
           .cust-carousel-h1{
               color: #24a1dc; 
            }
       }
   } 
   
   .cust-canvas-line{
       position: absolute;
       top:  0px;
       left: 0px;
       right: 0px;
       bottom: 0px;
       /*
       width: 100%;
       margin-left: 15px;
       margin-right: 15px;
       min-height: 7.5rem;*/
       z-index: -1;
       canvas{
           width: 100%;
           height: 100%;
       }
   }
    
   .carousel.slide{
       .carousel-indicators{
           li{
               border-color: transparent;
               background-color: #c0c0c0;
               &.active{
                   background-color: @color-orange
               }
           }
       }
       
       .carousel-inner{
       }
       
       .carousel-control{
           z-index: 999;
           
           >.glyphicon{
                border: 1px solid #FFFFFF;
                border-width: 0px 1px 1px 0px;
                &::before{
                    content: "";
                }
            }
            
            &.left{
               width: @carousel-pointer-width;
               left: -@carousel-pointer-width;
               span{
                transform: rotate(135deg);
                -ms-transform: rotate(135deg);
                -moz-transform: rotate(135deg);
                -webkit-transform: rotate(135deg);
                -o-transform: rotate(135deg);
               }
            }
            
            &.right{
               width: @carousel-pointer-width;
               right: -@carousel-pointer-width;
               span{
                transform: rotate(315deg);
                -ms-transform: rotate(315deg);
                -moz-transform: rotate(315deg);
                -webkit-transform: rotate(315deg);
                -o-transform: rotate(315deg);
                   
               }
            }
       }
   }
    
}



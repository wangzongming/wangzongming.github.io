html { font-family: "Microsoft YaHei","Î¢ÈíÑÅºÚ"; }
html, body { height: 100%; }
body { max-width: 1920px; margin: 0 auto; min-width: 1200px; }
a { color: inherit; text-decoration: none; }
input, textarea {    border: 0 none;padding:0;font-family: "Microsoft YaHei","Î¢ÈíÑÅºÚ"; }
textarea {resize:none}
::-webkit-input-placeholder{color:#8a8a89;font-family: "Microsoft YaHei","Î¢ÈíÑÅºÚ";}
.wrap { width: 100%; }
    .wrap h2 { font-size: 30px; color: #282828; padding: 40px 0; text-align: center; font-weight: normal; }
        .wrap h2 span { font-size: 20px; color: #5a5a5a; }
    .wrap .con { position: relative; }

.mid { text-align: center; }
    .mid:before { content: ""; width: .1em; height: 100%; display: inline-block; vertical-align: middle; }
    .mid > .mid_con { display: inline-block; vertical-align: middle; }

.clear { *zoom: 1; }
    .clear:after { content: ""; display: block; visibility: hidden; clear: both; line-height: 0; height: 0; }
.f_l { float: left; }
.f_r { float: right; }

.swiper-mybuttonBox { padding: 20px 0; }
    .swiper-mybuttonBox .swiper-mybutton-prev { user-select: none; display: inline-block; width: 30px; height: 30px; line-height: 27px; font-size: 25px; border-radius: 4px; text-align: center; color: #fff; background-color: #fc1330; cursor: pointer; }
    .swiper-mybuttonBox .swiper-mybutton-next { margin-left: 15px; user-select: none; display: inline-block; width: 30px; height: 30px; line-height: 27px; font-size: 25px; border-radius: 4px; text-align: center; color: #fff; background-color: #18171c; cursor: pointer; }
.mark { opacity: 0; position: absolute; left: 0; right: 0; top: 0; bottom: 0; background-color: rgba(253,55,16,.6); transition: opacity .3s; cursor: pointer; }
    .mark:hover { opacity: 1; }


.page { min-height: 100%; box-sizing: border-box; padding-bottom: 550px; position: relative; }


/*¶¥²¿*/
.hd { z-index: 10; position: relative; }
.hd_con { min-width: 1200px; margin: 0 auto; max-width: 1440px; font-size: 15px; color: #4f4d4d; }
    .hd_con .hd_logo { padding: 0 20px; height: 80px; }
        .hd_con .hd_logo > img:nth-child(2n+1) { padding-top: 10px; }
        .hd_con .hd_logo > img:nth-child(2n+2) { padding-top: 33px; margin-left: 5px; }
    .hd_con .navBtn { position: relative; text-align: center; }
        .hd_con .navBtn a { display: block; height: 80px; line-height: 90px; padding: 0 25px; }
        .hd_con .navBtn .subNav { position: absolute; top: 100%; left: 0; display: none; }
            .hd_con .navBtn .subNav li { border-top: 1px solid #ddd; }
            .hd_con .navBtn .subNav a { height: 46px; line-height: 46px; background-color: #fff;white-space:nowrap; }
    .hd_con .call { padding-left: 40px; height: 80px; line-height: 90px; background: url(../img/icon_call.png) no-repeat 10px 36px; }
    .hd_con .navBtn a:hover, .hd_con .navBtn a.on { background-color: #f04838; color: #fff; }

@media screen and (max-width: 1439px) {
    .hd_con .navBtn a { padding: 0 15px; }
}
/*µ×²¿*/
.ft { height: 550px; background-color: #1f2325; position: absolute; bottom: 0; left: 0; right: 0; }
/*126px*/
.ft_tit { font-size: 28px; color: #fd7a10; height: 95px; padding-top: 30px; border-bottom: 1px solid #b4b6b7; text-align: center; }
    .ft_tit > span { color: #b4b6b7; font-size: 10px; }
/*294px*/
.ft_nav { width: 1200px; margin: 0 auto; height: 254px; padding-top: 40px; }
    .ft_nav li { color: #b4b6b7; font-size: 12px;line-height: 2;  }
        .ft_nav li + li { padding-left: 20px;}
        .ft_nav li > dl { padding-left: 15px; padding-top: 20px;}
            .ft_nav li > dl dt { margin-bottom: 20px; font-size: 18px;line-height: 1; }
/*130px*/
.ft_copyright { height: 130px; line-height: 130px; text-align: center; background-color: #060708; color: #fff; font-size: 14px; }


/*Ê×Ò³Ä£¿é-banner*/
.index_banner .swiper-container { height: 650px; }
.index_banner .swiper-slide { background-repeat: no-repeat; background-position: center; position: relative; }
.index_banner .swiper-pagination-bullet { width: 12px; height: 12px; border-radius: 6px; background-color: #fff; opacity: 1; }
.index_banner .swiper-container-vertical > .swiper-pagination-bullets { right: 32px; }
    .index_banner .swiper-container-vertical > .swiper-pagination-bullets .swiper-pagination-bullet { margin: 12px 0; }
.index_banner .swiper-pagination-bullet-active { background-color: #b4b4b5; }
.index_banner .ban_con { width: 1200px; margin: 0 auto; height: 650px; }
    .index_banner .ban_con .ban_logo { width: 320px; height: 320px; border-radius: 160px; margin: 0 auto; background-color: #fff; }
        .index_banner .ban_con .ban_logo img { display: block; margin: 0 auto; }
    .index_banner .ban_con .ban_tit { font-size: 60px; margin-top: 20px; color: #fff; }
    .index_banner .ban_con .ban_tel { font-weight: bold; font-size: 25px; margin-top: 40px; color: #fff; }
    .index_banner .ban_con .ban_telicon { display: block; margin: 0 auto; margin-top: 10px; }
/*Ê×Ò³Ä£¿é-productCenter*/
.index_productCenter { }
    .index_productCenter .swiper-slide img { display: block; width: 100%; }
    .index_productCenter .swiper-mybuttonBox { text-align: center; }
    .index_productCenter .mark .mid_con { width: 80%; }
        .index_productCenter .mark .mid_con img { display: block; margin: 10px auto; width: auto; }
        .index_productCenter .mark .mid_con p { font-size: 16px; color: #fff; margin-top: 10px; line-height: 2; }
/*Ê×Ò³Ä£¿é-method*/
.index_method .con { background-repeat: no-repeat; background-position: center; background-image: url(../img/method.jpg); }
.index_method .method_con { width: 1200px; margin: 0 auto; height: 800px; position: relative; }
    .index_method .method_con .method_btnBox { position: absolute; left: 200px; }
        .index_method .method_con .method_btnBox li { margin: 0 10px; width: 120px; height: 120px; background-repeat: no-repeat; cursor: pointer; }
            .index_method .method_con .method_btnBox li:nth-child(4n+1) { background-image: url(../img/method_a1.png); }
            .index_method .method_con .method_btnBox li:nth-child(4n+2) { background-image: url(../img/method_a2.png); }
            .index_method .method_con .method_btnBox li:nth-child(4n+3) { background-image: url(../img/method_a3.png); }
            .index_method .method_con .method_btnBox li:nth-child(4n+4) { background-image: url(../img/method_a4.png); }
            .index_method .method_con .method_btnBox li.on:nth-child(4n+1) { background-image: url(../img/method_b1.png); }
            .index_method .method_con .method_btnBox li.on:nth-child(4n+2) { background-image: url(../img/method_b2.png); }
            .index_method .method_con .method_btnBox li.on:nth-child(4n+3) { background-image: url(../img/method_b3.png); }
            .index_method .method_con .method_btnBox li.on:nth-child(4n+4) { background-image: url(../img/method_b4.png); }
    .index_method .method_con .method_pBox { position: absolute; width: 700px; top: 610px; left: 50px; font-size: 18px; color: #fff; line-height: 2.5; }
        .index_method .method_con .method_pBox .method_p { display: none; }
    .index_method .method_con .method_imgBox { position: absolute; width: 296px; height: 526px; top: 140px; left: 816px; background-color: #f04838; overflow: hidden; }
        .index_method .method_con .method_imgBox .method_img { display: none; width: 100%; height: 100%; position: absolute; left: 0; top: 0; }
/*Ê×Ò³Ä£¿é-eaf*/
.index_eaf > img { display: block; width: 100%; margin: 0 auto; }
.index_eaf .con .f_l { width: 33.3333%; position: relative; }
    .index_eaf .con .f_l > img { display: block; width: 100%; }
.index_eaf .mark .mid_con { width: 60%; }
    .index_eaf .mark .mid_con img { display: block; margin: 10px auto; }
    .index_eaf .mark .mid_con h3 { font-size: 30px; color: #fff; font-weight: normal; }

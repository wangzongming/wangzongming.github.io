    .index_joinus .joinus_con .joinus_tit { font-size: 35px; margin-top: 40px; color: #fff; }
    .index_joinus .joinus_con .mid_con { position: relative; }
        .index_joinus .joinus_con .mid_con .joinus_l, .index_joinus .joinus_con .mid_con .joinus_r {  position: absolute; top: 50px; color: #fff; line-height: 2.5; text-align: left; }
    .index_joinus .joinus_con .mid_con .joinus_l { left: -200px; }     
    .index_joinus .joinus_con .mid_con .joinus_r { right: -150px; }
        .index_joinus .joinus_con .mid_con .joinus_img{ margin-right: 15px; }
        .index_joinus .joinus_con .mid_con .joinus_t{ font-size: 17px; cursor: pointer; }	
	.index_joinus .joinus_con .mid_con .joinus_ul{opacity: 0; transition: opacity .3s;}
        .index_joinus .joinus_con .mid_con .joinus_ul li{ font-size: 12px; border-top: 1px solid #fff; cursor: pointer; }
    	.index_joinus .joinus_con .joinus_lr:hover .joinus_ul { opacity: 1; }

/*Ê×Ò³Ä£¿é-patented*/
.index_patented { background-color: #f5f5f5; }

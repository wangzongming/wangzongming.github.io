.index_eaf .mark:hover .mid_con p { color: #f5f5f5; }

/*Ê×Ò³Ä£¿é-joinus*/
.index_joinus .con { background-repeat: no-repeat; background-position: center; background-image: url(../img/joinus.jpg); }
.index_joinus .joinus_con { width: 1200px; margin: 0 auto; height: 444px; }
    .index_joinus .joinus_con .joinus_logo { width: 240px; height: 240px; border-radius: 120px; margin: 0 auto; background-color: #fff; }

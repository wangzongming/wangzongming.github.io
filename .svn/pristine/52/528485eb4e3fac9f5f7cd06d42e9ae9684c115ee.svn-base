.contactBanner {background-repeat: no-repeat; background-position: center; height: 400px; background-color: #f2f2f2; }

/*contentContainer*/
.contentContainer { width: 1200px; margin: 0 auto; position: relative; padding-bottom: 20px; }
    .contentContainer .conNav { position: relative; width: 250px; }
        .contentContainer .conNav .conNav_tit { position: absolute; top: -90px; left: 0; font-size: 23px; color: #fff; padding-left: 40px; width: 230px; line-height: 100px; height: 90px; background-color: #f04838; }
        .contentContainer .conNav .conNav_ul { min-height: 355px; border-left: 10px solid #b8b9bd; border-bottom: 1px solid #b8b9bd; background-color: #f4f4f4; padding-bottom: 20px; }
            .contentContainer .conNav .conNav_ul li { padding: 0 20px 0 30px; position: relative; }
            .contentContainer .conNav .conNav_ul:after { content: ""; position: absolute; right: -20px; top: -1px; width: 20px; height: 100%; background-color: #f4f4f4; display: block; border-bottom: 1px solid #b8b9bd; }
            .contentContainer .conNav .conNav_ul li.on:before, .contentContainer .conNav .conNav_ul li:hover:before { content: ""; position: absolute; left: -10px; top: 0; width: 10px; height: 100%; background-color: #f04838; display: block; }
            .contentContainer .conNav .conNav_ul li a { font-size: 19px; color: #282828; height: 70px; line-height: 70px; display: block; border-bottom: 1px solid #a8a8aa; }
    .contentContainer .con { position: relative; top: -50px; width: 848px; padding: 50px; border: 1px solid #c9c9c9; background-color: #fff; }
        .contentContainer .con .con_tit { height: 40px; line-height: 40px; border-bottom: 1px solid #c9c9c9; }
            .contentContainer .con .con_tit > span { font-size: 19px; color: #282828; }
            .contentContainer .con .con_tit .trail { font-size: 16px; line-height: 45px; color: #979898; text-align: center; }
                .contentContainer .con .con_tit .trail a { padding-left: 20px; }
        .contentContainer .con .con_pre { padding: 20px 0; }

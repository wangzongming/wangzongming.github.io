body,
html {
	min-width: 1200px;
	font-family: Arial, Verdana, "微软雅黑", "宋体";
	color: #000;
	font-size: 12px;
	line-height: 1.8;
	-webkit-text-size-adjust: none;
}
p, ul, h3, h4, dl {
	margin: 0;
	padding: 0;
}
h3, h4 {
	font-weight: normal;
}
a {
	text-decoration: none;
	color: inherit;
}
li {
	list-style: none;
}
.fl {
	float: left;
}
.fr {
	float: right;
}
.clearfix {
	zoom: 1;
}
.clearfix:after {
	visibility: hidden;
	display: block;
	font-size: 0;
	content: " ";
	clear: both;
	height: 0;
}
.rel {
	position: relative;
	width: 1340px;
	margin: 0 auto;
}
/*头部*/
.wrapper {
	width: 1200px;
	margin: 0 auto;
}
.top_nav li {
	margin-left: 47px;
	padding-left: 26px;
	line-height: 30px;
	height: 30px;
	color: #666666;
}
.phone {
	background: url(../img/top_nav3.png) no-repeat left center;
}
.weibo {
	background: url(../img/top_nav1.png) no-repeat left center;
}
.weixin {
	background: url(../img/top_nav2.png) no-repeat left center;
}
.nav {
	background: #1e326d;
}
.nav_li {
	width: 150px;
	text-align: center;
	color: #fff;
	padding-top: 30px;
	height: 90px;
	box-sizing: border-box;
	cursor:pointer;
	position:relative;
}
.nav_li.on,.nav_li:hover {
	background: #cc0001;
}
/*下拉菜单*/
.down-menu{
    width:100%;
    background:rgba(204,0,0,.6);   
    padding:0 15px;
	padding-bottom:32px;
    box-sizing: border-box;
    position: absolute;
    z-index: 50;
    top:90px;
    left:0;
    display: none;
    font-size:14px;
	color:#fff;
}
.down-menu ul li{
    line-height:38px;
    list-style: none;
    border-bottom:1px solid #dd9093;
    text-align:center;
}
.down-menu ul li a{
    color:white;
    text-decoration:none;
}
.ch {
	font-size: 16px;
	letter-spacing: 2px;
}
.banner {
	border-bottom: 2px solid #bfbfbf;
}
.banner img {
	width: 100%;
}
.haishen {
	padding-top: 30px;
	padding-bottom: 40px;
}
.swiper-slide {
	position: relative;
	width: 230px;
	height: 152px;
	cursor: pointer;
}
.haishen .swiper-slide.on .mask, .haishen .swiper-slide:hover .mask {
	display: block;
}
.haishen_img {
	border-radius: 7px;
	width: 230px;
	height: 152px;
}
.mask {
	position: absolute;
	left: 0;
	right: 0;
	top: 0;
	bottom: 0;
	background: rgba(0,0,0,.6);
	z-index: 2;
	color: #fff;
	text-align: center;
	padding-top: 16px;
	box-sizing: border-box;
	border-radius: 7px;
	display: none;
}
.pro_name {
	font-size: 24px;
	padding-top: 10px;
	padding-bottom: 8px;
}
.pro_eng_name {
	font-size: 14px;
}
.swiper-button-next.red_next_btn, .swiper-button-prev.red_prev_btn,.swiper-button-next.black_next_btn, .swiper-button-prev.black_prev_btn  {
	width: 40px;
	height: 88px;
	top: 60px;
	margin-top: 0;
}
.swiper-button-next.black_next_btn, .swiper-button-prev.black_prev_btn {
	top:130px;
}
.swiper-button-next.red_next_btn {
	background: url(../img/right.jpg) no-repeat;
	right: 0;
}
.swiper-button-prev.red_prev_btn {
	background: url(../img/left.jpg) no-repeat;
	left: 0;
}
.swiper-button-next.black_next_btn {
	background:url(../img/right_grey.png) no-repeat;
	right: 0;
}
.swiper-button-prev.black_prev_btn {
	background: url(../img/left_grey.png) no-repeat;
	left: 0;
}
.quality {
	background: url(../img/long_bg.jpg) no-repeat;
	background-size: 100% 100%;
}
.quality_l, .quality_r {
	width: 50%;
	height: 176px;
}
.quality_l {
	color: #fff;
	padding-right: 185px;
	text-align: right;
	box-sizing: border-box;
	background: url(../img/long_icon.png) no-repeat right 64px center;
}
.quality_r {
	padding-left: 185px;
	text-align: left;
	box-sizing: border-box;
	background: url(../img/long_icon2.png) no-repeat left 64px top 40px;
}
.quality_title {
	font-weight: bold;
	font-size: 36px;
	padding-top: 40px;
	line-height: 36px;
	padding-bottom: 15px;
}
.slogan {
	font-size: 18px;
	line-height: 26px;
}
.headline {
	text-align: center;
	padding-top: 36px;
	padding-bottom: 25px;
}
.headline_grey {
	font-size: 22px;
	color: #999999;
}
.headline_content{
	line-height:42px;
}
.content {
	font-size: 32px;
	color: #666;
	letter-spacing: 2px;
	padding: 3px 14px;
	position: relative;
}
.line_l, .line_r {
	position: absolute;
	top: 50%;
}
.line_l {
	left: -304px;
}
.line_r {
	right: -304px;
}
.group1 li, .group2 li {
	position: relative;
	margin-bottom: 10px;
	cursor: pointer;
}
.group1 li img, .group2 li img {
	width: 100%;
}
.group1 li, .shop .swiper-slide {
	width: 595px;
	height: 440px;
}
.group2 li {
	width: 392px;
	height: 440px;
}
.group1 li+li, .group2 li+li {
	margin-left: 10px;
}
.mask_pro {
	padding: 16px;
	background: rgba(255,255,255,.6);
	position: absolute;
	top: 50%;
	left: 50%;
	margin-left: -188px;
	color: #0f0f0f;
	margin-top: -112px;
	display: none;
}
.group1 li.on .mask_pro, .group2 li.on .mask_pro, .group1 li:hover .mask_pro, .group2 li:hover .mask_pro, .shop .swiper-slide.on .mask_pro, .shop .swiper-slide:hover .mask_pro {
	display: block;
}
.border {
	border: 1px solid #625755;
	text-align: center;
	width: 342px;
	padding-bottom: 20px;
}
.brand {
	height: 30px;
	line-height: 30px;
	padding-top: 30px;
	padding-bottom: 14px;
}
.brand span {
	position: relative;
	font-size: 24px;
}
.brand span:before {
	content: "";
	position: absolute;
	top: 15px;
	right: -36px;
	width: 30px;
	height: 1px;
	background: #0f0f0f;
}
.brand span:after {
	content: "";
	position: absolute;
	top: 15px;
	left: -36px;
	width: 30px;
	height: 1px;
	background: #0f0f0f;
}
.name {
	font-size: 32px;
	font-weight: bold;
	letter-spacing: 5px;
	margin-bottom: 24px;
}
.more {
	font-size: 18px;
	padding: 2px 10px 2px 18px;
	border: 1px solid #0f0f0f;
	border-radius: 15px;
	display: block;
	width: 160px;
	margin: 0 auto;
	letter-spacing: 3px;
	white-space: nowrap;
	background: transparent;
}
.net {
	background: #c90000;
}
.net_data {
	background: url(../img/red_bg.jpg) no-repeat;
	background-size: 100% 100%;
	color: #fff;
	font-size: 28px;
	padding-top: 20px;
	padding-bottom: 50px;
}
.net_top_ul li {
	font-size: 50px;
	line-height: 77px;
	height: 77px;
	width: 180px;
	text-align: center;
}
.net_top_ul li+li {
	border-left: 1px solid #fff;
}
.unit {
	font-size: 24px;
	margin-left: 18px;
}
.net_top p {
	line-height: 77px;
	height: 77px;
}
.count {
	font-size: 170px;
	text-align: center;
	margin-top: 10px;
	margin-bottom: 35px;
}
.net_data .more {
	width: 343px;
	border-color: #fff;
	font-size: 28px;
	border-radius: 22px;
}
.shop {
	margin-bottom: 60px;
}
.shop .swiper-slide {
	position: relative;
}
.shop .mask_pro {
	color: #fff;
	background: rgba(0,0,0,.4);
}
.shop .more {
	border-color: #fff;
}
.shop .name {
	font-size: 21px;
	font-weight: normal;
	padding-top: 5px;
	margin-bottom: 20px;
}
.shop .brand {
	font-size: 32px;
	letter-spacing: 2px;
}
#haishen_next_btn1, #haishen_prev_btn1 {
	top: 340px;
}
.border {
	padding-bottom: 30px;
	border-color: #b8b0a1;
}
.service_img {
	margin: 0 auto;
	margin-top: 5px;
	margin-bottom: 40px;
	display: block;
}
.service {
	font-size: 18px;
	color: #666;
	text-align: center;
	line-height: 30px;
}
.video_box {
	margin-top: 30px;
}
.swf_r {
	width: 100%;
	height: auto;
	position: relative;
}
.video_box img {
	display: block;
	width: 100%;
	height: 100%;
}
.video_box .fancybox_swf {
	transition: all 0.12s;
	background-color: rgba(0, 0, 0, 0.38);
	position: absolute;
	display: block;
	width: 100%;
	height: 100%;
	left: 0;
	top: 0;
	background-repeat: no-repeat;
	background-position: center center;
	cursor: pointer;
	display: block;
}
.video_box .fancybox_swf:hover {
	background-color: rgba(0, 0, 0, 0.68);
}
.video_box .swf_r .fancybox_swf {
	background-image: url(../img/frame.png);
	background-position: center center;
	background-size: 1200px 576px;
}
.news_list_li {
	width: 390px;
	height: 380px;
	background: #666666;
	position: relative;
}
.news_list li+li {
	margin-left: 15px;
}
.news_content_dl {
	padding: 0 18px;
	padding-top: 15px;
	color: #fff;
	font-size: 16px;
}
.time {
	line-height:20px;
	height:20px;
	margin-bottom: 7px;
}
.about {
	margin-bottom: 10px;
	font-size: 18px;
	line-height:22px;
	height:22px;
}
.detail_content {
	line-height: 22px;
	max-height: 44px;
	overflow: hidden;
}
.more_detail {
	font-size: 14px;
	margin-top: 4px;
	cursor: pointer;
}
.tri {
	width: 0;
	height: 0;
	border-bottom: 30px solid #cc0000;
	border-left: 30px solid transparent;
	position: absolute;
	bottom: 0;
	right: 0;
}
.news_list_li:hover {
	background: #cc0000;
}
.news_list_li:hover .tri {
	border-bottom-color: #ffba1e;
}
.news_more {
	color: #999999;
	font-size: 24px;
	width: 160px;
	height: 40px;
	line-height: 40px;
	text-align: center;
	border: 1px solid #999;
	margin: 50px auto;
	display: block;
	cursor:pointer;
}

.evaluate{
	background:url(../img/feedback_bg.jpg) no-repeat;
	background-size: 100% 100%;
	padding-bottom:100px;
}
.evaluate .content{ color:#fff;}
.customer .swiper-slide{ width:156px; height:270px;position:relative;}
.show_img_con{ width:154px; height:154px; border:1px solid #fff; border-radius:50%; text-align:center; position:absolute;
top:0; left:0; z-index:2;}
.show_img{ width:128px; height:128px; border:4px solid #676767; padding:2px; background:#fff; border-radius:50%; margin:14px auto; text-align:center; box-sizing:border-box;}
.show_img img{ width:112px; height:112px; border-radius:50%; margin:2px 0;}
.message{ color:#666666; font-size:18px; line-height:26px; max-height:104px; overflow:hidden; text-align:justify; text-indent:1.5em;}
.message_con{ background:#fff; position:absolute; top:77px; height:187px; width:100%;padding:0 10px; padding-top:74px; box-sizing:border-box;}
.swiper-slide_item{ position:relative;width:156px; height:270px;}

/*底部start*/
footer {
	background: #1e326d;
	margin-top: 50px;
}
/*主要底部内容*/
.footer-container {
	width: 1200px;
	margin: 0 auto;
	padding: 15px 0;
}
.footer-container::after {
	content: "";
	display: table;
	clear: both;
	overflow: hidden;
}
.footer-left {
	float: left;
	border-right: 1px solid #63709a;
	padding: 50px 100px 45px 0;
}
.footer-left ul {
	float: left;
	margin-right: 80px;
}
.footer-left ul li a {
	text-decoration: none;
	color: rgba(255,255,255,0.5);
}
.footer-left ul li {
	list-style: none;
	font-size: 10px;
	padding: 3px;
	box-sizing: border-box;
}
.footer-left ul li h3 {
	font-size: 16px;
	color: rgba(255,255,255,0.9);
}
.footer-right {
	float: right;
	color: #fff;
}
.footer-title {
	font-size: 18px;
	padding-top: 12px;
}
.footer_contact {
	text-align: center;
	margin-top: 12px;
}
.footer_contact p {
	font-size: 14px;
	text-align: center;
	margin-top: 10px;
}
.footer_contact li+li {
	margin-left: 23px;
}
.footer_contact img {
	width: 150px;
	height: 150px;
	border-radius: 10px;
	vertical-align: middle;
}
/*footer 上的黑色部分*/
.footer-bottom {
	height: 40px;
	background: black;
}

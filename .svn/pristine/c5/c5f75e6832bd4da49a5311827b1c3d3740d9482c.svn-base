@charset "utf-8";
html { overflow-y: scroll; }

body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, form, fieldset, input, textare a, p, blockquote, th, td { margin: 0; padding: 0; }

address, caption, cite, code, dfn, em, strong, th, var { font-style: normal; }

table { border-collapse: collapse; border-spacing: 0; }

caption, th { text-align: left; }

q:before, q:after { content: ''; }

object, embed { vertical-align: top; }

hr, legend { display: none; }

h1, h2, h3, h4, h5, h6 { font-size: 100%; }

img, abbr, acronym, fieldset { border: 0; }

ol li, ul li { list-style-type: none; }

a, label { cursor: pointer; }

a { text-decoration: none; color: #333; }

.inner { width: 1200px; margin: 0 auto; }

body { font-family: "微软雅黑", Arial, Helvetica, sans-serif; font-size: 12px; color: #333333; overflow: hidden; }

.contain { width: 100%; height: 100%; display: block; position: relative; z-index: 1; background: #f8f8f8; }

.header { box-shadow: 0 0 30px rgba(33, 33, 33, .3); background: #FFF url(../img/bg-header.gif) top repeat-x; height: 135px; min-width: 1200px; position: fixed; width: 100%; z-index: 999; }

    .header .top { height: 30px; line-height: 30px; color: #FFF; position: relative; }

        .header .top dl { position: absolute; right: -26px; width: 105px; background: #FFF; border-bottom: #399e52 4px solid; padding: 13px 29px 16px; display: none; box-shadow: 0 4px 4px #999; }

            .header .top dl dt { color: #333; line-height: 12px; height: 12px; border-left: #399e52 2px solid; padding-left: 5px; }

            .header .top dl dd { padding-top: 9px; }
        .header .top span { float: right; }

            .header .top span em { padding-left: 25px; cursor: pointer; }

            .header .top span a { color: #fff; }

            .header .top span em.collection { background: url(../img/bg-header_Collection.png) no-repeat left; padding-right: 15px; }

            .header .top span em.weixin { background: url(../img/bg-header_weixin.png) no-repeat 10px center; display: inline-block; margin-left: 5px; padding-right: 20px; padding-left: 35px; }

                .header .top span em.weixin strong { font-weight: normal; background: url(../img/bg-header_weixin_more.png) no-repeat right; padding-right: 20px; }

                .header .top span em.weixin.current { color: #399e52; background: #FFF url(../img/bg-header_weixin_hover.png) no-repeat 10px center; padding-right: 20px; padding-left: 35px; margin-left: 5px; }

                    .header .top span em.weixin.current strong { background: url(../img/bg-header_weixin_more_hover.png) no-repeat right; padding-right: 20px; }

    .header .inner:after { content: "."; visibility: hidden; display: block; height: 0; clear: both; }

    .header .logo { float: left; height: 105px; }

    .header .logo_img { cursor: pointer; }

    .header .nav { float: right; padding-right: 20px; }

        .header .nav ul { padding-top: 42px; }

            .header .nav ul li { float: left; text-align: center; font-size: 15px; padding: 0 28px; }

                .header .nav ul li + li { background: url(../img/nav_bar.gif) no-repeat left 2px; }

                .header .nav ul li span { display: block; font-size: 10px; padding-top: 5px; }

                .header .nav ul li.current a,
                .header .nav ul li a:hover { color: #399e52; }

.contactus { min-width: 1200px; padding-top: 130px; }

    .contactus .inner { height: auto; position: relative; }

    .contactus h2 { text-align: center; padding-top: 46px; border-bottom: 1px solid #a0a0a0; position: relative; margin-bottom: 2px; }

        .contactus h2 .h2_bar { background: url(../img/bg-h2.jpg) no-repeat center bottom; position: absolute; width: 100%; height: 5px; left: 0; bottom: -3px; }

        .contactus h2 strong { font-weight: normal; display: inline-block; }

            .contactus h2 strong span:first-child { display: block; font-size: 22px; line-height: 1; height: 26px; }

            .contactus h2 strong span:last-child { display: block; font-size: 16px; line-height: 1; height: 22px; }

    .contactus h3 { border-bottom: 1px solid #a0a0a0; position: relative; margin-bottom: 2px; line-height: 50px; height: 47px; }

        .contactus h3 .h3_bar { background: url(../img/bg-h2.jpg) no-repeat left bottom; position: absolute; width: 100%; height: 5px; left: 0; bottom: -3px; }

        .contactus h3 strong { font-weight: normal; display: inline-block; }
            .contactus h3 strong span:first-child { font-size: 22px; }

            .contactus h3 strong span:last-child { font-size: 20px; line-height: 50px; color: #03a75f; }

        .contactus h3 .con_tit { font-size: 17px; font-weight: normal; color: #000; }

        .contactus h3 .h3_right { float: right; font-size: 14px; font-weight: normal; color: #000; line-height: 55px; }

.footer { background: #399e52; line-height: 140px; min-width: 1200px; color: #FFF; text-align: center; display: block; position: inherit; }
    .footer .inner { background: #399e52; width: 100%; }
.payer { background: rgba(144, 143, 142, .7); width: 100%; height: 100%; top: 0; bottom: 0; position: fixed; z-index: 9999; display: none; }
    .payer .down_list { width: 1024px; height: 360px; background: #FFF; top: 215px; left: 50%; margin-left: -512px; position: absolute; box-shadow: #666 0px 0px 5px 1px; }
        .payer .down_list ul { padding-left: 45px; }
            .payer .down_list ul:after { content: "."; visibility: hidden; display: block; height: 0; clear: both; }
        .payer .down_list li { width: 150px; float: left; font-size: 18px; text-align: center; height: 127px; padding-top: 23px; margin-top: 105px; margin-left: 25px; transition: background 1s; -moz-transition: background 1s; -webkit-transition: background 1s; -o-transition: background 1s; background: url(../img/circle_150_hover.png) no-repeat center; }
            .payer .down_list li a { color: #999999; }
            .payer .down_list li:hover { background: url(../img/circle_150.png) no-repeat center; }
                .payer .down_list li:hover a { color: #399e52; }
            .payer .down_list li span { padding-top: 80px; display: block; transition: background 1s; -moz-transition: background 1s; -webkit-transition: background 1s; -o-transition: background 1s; }
            .payer .down_list li.last { margin-top: 50px; margin-left: 35px; background: none; height: auto; width: 181px; height: 223px; background: url(../img/ico-pr_down.png) no-repeat center 23px; }
                .payer .down_list li.last em { display: block; width: 181px; height: 223px; }
                    .payer .down_list li.last em.index_1 { animation: index_1 1.2s linear; -webkit-animation: index_1 1.2s linear; }
                    .payer .down_list li.last em.index_2 { animation: index_2 1.2s linear; -webkit-animation: index_2 1.2s linear; }
                    .payer .down_list li.last em.index_3 { animation: index_3 1.2s linear; -webkit-animation: index_3 1.2s linear; }
                    .payer .down_list li.last em.index_4 { animation: index_4 1.2s linear; -webkit-animation: index_4 1.2s linear; }

@keyframes index_1 {
    0% { opacity: 0; }
    100% { opacity: 1; }
}

@-webkit-keyframes index_1 {
    0% { opacity: 0; }
    100% { opacity: 1; }
}

@keyframes index_2 {
    0% { opacity: 0; }
    100% { opacity: 1; }
}

@-webkit-keyframes index_2 {
    0% { opacity: 0; }
    100% { opacity: 1; }
}

@keyframes index_3 {
    0% { opacity: 0; }
    100% { opacity: 1; }
}

@-webkit-keyframes index_3 {
    0% { opacity: 0; }
    100% { opacity: 1; }
}

@keyframes index_4 {
    0% { opacity: 0; }
    100% { opacity: 1; }
}

@-webkit-keyframes index_4 {
    0% { opacity: 0; }
    100% { opacity: 1; }
}

.payer .down_list li.last em.index_1 { background: url(../img/pr-iphone.png) no-repeat center 14px; }
.payer .down_list li.last em.index_2 { background: url(../img/pr-Android.png) no-repeat center 14px; }
.payer .down_list li.last em.index_3 { background: url(../img/pr-web.png) no-repeat center 14px; }
.payer .down_list li.last em.index_4 { background: url(../img/pr-WeChat.png) no-repeat center 14px; }
.payer .down_list li.down_1 span { background: url(../img/ico-iphone.png) no-repeat center top; }
.payer .down_list li.down_2 span { background: url(../img/ico-and.png) no-repeat center top; }
.payer .down_list li.down_3 span { background: url(../img/ico-web.png) no-repeat center top; }
.payer .down_list li.down_4 span { background: url(../img/ico-wechat.png) no-repeat center top; }
.payer .down_list li.down_1:hover span { background: url(../img/ico-iphone_hover.png) no-repeat center top; }
.payer .down_list li.down_2:hover span { background: url(../img/ico-and_hover.png) no-repeat center top; }
.payer .down_list li.down_3:hover span { background: url(../img/ico-web_hover.png) no-repeat center top; }
.payer .down_list li.down_4:hover span { background: url(../img/ico-wechat_hover.png) no-repeat center top; }
.payer .down_list p { position: absolute; top: -18px; right: -17px; }
.animation_open { -webkit-animation: bounceIn .5s .2s ease both; -moz-animation: bounceIn .5s .2s ease both; }

@-webkit-keyframes bounceIn {
    0% { opacity: 0; -webkit-transform: scale(.3); }
    100% { opacity: 1; -webkit-transform: scale(1); }
}

@-moz-keyframes bounceIn {
    0% { opacity: 0; -moz-transform: scale(.3); }
    100% { opacity: 1; -moz-transform: scale(1); }
}

.animation_close { -webkit-animation: bounceOut .5s .2s ease both; -moz-animation: bounceOut .5s .2s ease both; }

@-webkit-keyframes bounceOut {
    0% { opacity: 1; -webkit-transform: scale(1); }
    100% { opacity: 0; -webkit-transform: scale(.3); }
}

@-moz-keyframes bounceOut {
    0% { opacity: 1; -moz-transform: scale(1); }
    100% { opacity: 0; -moz-transform: scale(.3); }
}

.none { display: none; }
.loading { width: 100%; height: 100%; position: absolute; z-index: 99999; }
#floatingCirclesG { position: relative; width: 128px; height: 128px; -webkit-transform: scale(0.6); -moz-transform: scale(0.6); -o-transform: scale(0.6); -ms-transform: scale(0.6); left: 50%; margin-left: -64px; top: 50%; margin-top: -64px; }
.f_circleG { position: absolute; background-color: #FFFFFF; height: 23px; width: 23px; -moz-border-radius: 12px; -webkit-border-radius: 12px; border-radius: 12px; -webkit-animation-name: f_fadeG; -webkit-animation-duration: 1.6s; -webkit-animation-iteration-count: infinite; -webkit-animation-direction: linear; -moz-animation-name: f_fadeG; -moz-animation-duration: 1.6s; -moz-animation-iteration-count: infinite; -moz-animation-direction: linear; -o-animation-name: f_fadeG; -o-animation-duration: 1.6s; -o-animation-iteration-count: infinite; -o-animation-direction: linear; -ms-animation-name: f_fadeG; -ms-animation-duration: 1.6s; -ms-animation-iteration-count: infinite; -ms-animation-direction: linear; }
#frotateG_01 { left: 0; top: 52px; -webkit-animation-delay: 0.6s; -moz-animation-delay: 0.6s; -o-animation-delay: 0.6s; -ms-animation-delay: 0.6s; }
#frotateG_02 { left: 15px; top: 15px; -webkit-animation-delay: 0.8s; -moz-animation-delay: 0.8s; -o-animation-delay: 0.8s; -ms-animation-delay: 0.8s; }
#frotateG_03 { left: 52px; top: 0; -webkit-animation-delay: 1s; -moz-animation-delay: 1s; -o-animation-delay: 1s; -ms-animation-delay: 1s; }
#frotateG_04 { right: 15px; top: 15px; -webkit-animation-delay: 1.2s; -moz-animation-delay: 1.2s; -o-animation-delay: 1.2s; -ms-animation-delay: 1.2s; }
#frotateG_05 { right: 0; top: 52px; -webkit-animation-delay: 1.4s; -moz-animation-delay: 1.4s; -o-animation-delay: 1.4s; -ms-animation-delay: 1.4s; }
#frotateG_06 { right: 15px; bottom: 15px; -webkit-animation-delay: 1.6s; -moz-animation-delay: 1.6s; -o-animation-delay: 1.6s; -ms-animation-delay: 1.6s; }
#frotateG_07 { left: 52px; bottom: 0; -webkit-animation-delay: 1.8s; -moz-animation-delay: 1.8s; }
#frotateG_08 { left: 15px; bottom: 15px; -webkit-animation-delay: 2s; -moz-animation-delay: 2s; -o-animation-delay: 1.8s; -ms-animation-delay: 1.8s; }
#frotateG_08 { left: 15px; bottom: 15px; -webkit-animation-delay: 2s; -moz-animation-delay: 2s; -o-animation-delay: 2s; -ms-animation-delay: 2s; }

@-webkit-keyframes f_fadeG {
    0% { background-color: #000000; }
    100% { background-color: #FFFFFF; }
}

@-moz-keyframes f_fadeG {
    0% { background-color: #000000; }
    100% { background-color: #FFFFFF; }
}

@-o-keyframes f_fadeG {
    0% { background-color: #000000; }
    100% { background-color: #FFFFFF; }
}

@-ms-keyframes f_fadeG {
    0% { background-color: #000000; }
    100% { background-color: #FFFFFF; }

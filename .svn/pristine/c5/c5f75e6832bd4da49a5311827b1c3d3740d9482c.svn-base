/* services.css */

.services { padding: 50px 78px 20px; position: relative; }
    .services .swiper-wrapper:after { content: "."; visibility: hidden; display: block; height: 0; clear: both; }
    .services .swiper-slide { box-sizing: border-box; height: 370px; border: 1px solid #399e52; /*background: url(../img/circle_142.png) no-repeat center 30px;*/ z-index: 1; position: relative; }
        .services .swiper-slide,
        .services .swiper-slide.current::after,
        .services .swiper-slide::after { -webkit-transition: all 0.2s; -moz-transition: all 0.2s; -o-transition: all 0.2s; transition: all 0.2s; }
            .services .swiper-slide.current::before,
            .services .swiper-slide.current::after,
            .services .swiper-slide::before,
            .services .swiper-slide::after { background: url(../img/green_bg.png) top center no-repeat; content: ''; position: absolute; z-index: -1; background-size: 100% 100%; }
            /* BUTTON 1 */

            .services .swiper-slide.current::after,
            .services .swiper-slide::after { height: 0; left: 0; top: 0; width: 100%; }
            .services .swiper-slide.current:hover::after,
            .services .swiper-slide:hover:after { height: 100%; }
            .services .swiper-slide.mr_no { margin-right: 0; }
            .services .swiper-slide dt { width: 100%; height: 195px; }
            .services .swiper-slide dd.title { color: #399e52; font-size: 18px; text-align: center; }
            .services .swiper-slide dd.con { line-height: 18px; padding: 15px 10px 0; }

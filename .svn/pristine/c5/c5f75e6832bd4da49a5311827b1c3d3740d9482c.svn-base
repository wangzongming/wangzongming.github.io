.window_close { cursor: pointer; width: 50px; height: 50px; position: absolute; right: 18px; top: 20px; background: url(../img/close2.png); }
/*partner.css*/

.partner { padding: 50px 78px 20px; }
    .partner .swiper-container { }
    .partner .swiper-wrapper { border-top: 1px solid #959595; border-left: 1px solid #959595; }
    .partner .swiper-slide { box-sizing: border-box; cursor: pointer; overflow: hidden; text-align: center; line-height: 180px; height: 180px; background-color: #fff; border-bottom: 1px solid #959595; border-right: 1px solid #959595; }

body{
    background-color: #e8ebf0;
}
.formBox{
    padding-top:10px;
    position: relative;
}
.formItem{
    margin-bottom: 15px;
    margin-right: 15px;
   
}
.formItem label{
    height: 32px;
    line-height: 32px;
    margin-right:10px;
}
.formItem input{
    height: 32px;
    margin-right:10px;
    line-height: 32px;
    margin:0;
    padding:0 5px;
    box-sizing: border-box;
    border:1px solid #ddd;
}
.formItem select{
    height: 32px;
    margin-right:10px;
    line-height: 32px;
    margin:0;
    padding:0 10px;
    box-sizing: border-box;
    border:1px solid #ddd;
}
.formItem select option{
    line-height: 30px;
}
.formItem button{
    height: 32px;
    margin-right:10px;
    line-height: 32px;
    margin:0;
    padding:0 15px;
    box-sizing: border-box;
    background-color: #3cb3fc;
    color: #fff;
    border:1px solid #3cb3fc;
    cursor: pointer;
}
.formItem button.btn-default{
    background-color: #fff;
    color: #3cb3fc;
    border:1px solid #3cb3fc;
}

                        .tableBox {
                            border: 1px solid #ddd
                        }

                        .table {
                            border-collapse: collapse;
                            border-spacing: 0;
                            table-layout: fixed;
                            width: 100%;
                            position: relative;
                        }

                        .thead {
                            background-color: #f4f4f4
                        }

                        .th {
                            color: #666;
                            height: 40px;
                            padding: 0 10px;
                        }

                        .td {
                            height: 40px;
                            padding: 0 10px;
                            border-top: 1px solid #ddd
                        }




.page{ 
    min-width:900px; 
    margin:10px;
    position: relative;
}
.page-l{
    padding-right:340px; 
}
.page-r{
    position: absolute;
    top:0;
    right:0;
    width:320px;
}
.panel{
    box-sizing: border-box;
    background-color: white;
    box-shadow: 1px 1px 1px rgba(213,213,213,.6)
}
.panel-h{
    font-size: 14px;
    font-weight: bold;
    line-height: 20px;
    height: 20px;
    padding:15px 15px 10px;
    margin: 0;
    position: relative;
}
.panel-h .h-more{
    display: block;
    cursor: pointer;
    position: absolute;
    font-size: 12px;
    right:16px;
    top: 16px;
    font-weight: normal
}
.panel-n{
    height: 45px;
    background-color: #f4f4f4;  
}
.panel-n li{
    width: 25%;
    height: 45px;
    line-height: 45px;
    text-align: center;
    position: relative;
    box-sizing:  border-box;
    cursor: pointer;
    font-size: 14px;
    font-weight: bold;
    -webkit-transition: all .3s;
    transition: all .3s; 
}
.panel-n li::after{
    content: "";
    display: block;
    width:80%;
    height:2px;
    position: absolute;
    background: transparent;
    left:10%;
    bottom:0;
    -webkit-transition: all .3s;
    transition: all .3s; 
}
.panel-n li + li::before{
    content: "";
    display: block;
    width:1px;
    height:15px;
    position: absolute;
    background: #ddd;
    top:15px;
    left:0;
}
.panel-n li.cur,.panel-n li:hover{
    color:#3cb3fc;
}
.panel-n li.cur::after,.panel-n li:hover::after{
    background: #3cb3fc;
}
.panel-c{
    padding:13px 15px;
    font-size: 14px;
    position: relative;
}





/*统计*/
.tj-hd{
    position: relative;
}
.tj-hd-l{
    padding-right:260px;
}
.tj-hd-r{
    position: absolute;
    top:0;
    right:0;
    width:240px;
}


/*账号*/
.tj-zh,.tj-fs,.tj-db{
    height: 122px;
    box-sizing: border-box;
}
.tj-zh li,.tj-fs li,.tj-db li{
    height: 38px;
    line-height: 38px;
    background-color:#f4f4f4; 
}
.tj-zh li+li,.tj-fs li+li,.tj-db li+li{
    margin-top: 4px;
}
.tj-zh{
    padding:10px 0 0 30px;
    background-color:#2f2f2f; 
}


/*粉丝*/
.tj-fs li div:nth-child(2n+1){
    box-sizing: border-box;
    width:40%;
    text-align: right;
}
.tj-fs li div:nth-child(2n){
    box-sizing: border-box;
    padding-left:50px;
    width:60%;
}
.tj-fs li div:nth-child(2n) .num{
    font-size: 24px;
    margin-right: 5px;
}


/*待办*/
.tj-db li div:nth-child(2n+1){
    box-sizing: border-box;
    width:50%;
    padding-left:20px;
    font-size: 12px;
}
.tj-db li div:nth-child(2n){
    box-sizing: border-box;
    width:50%;
}
.tj-db li div:nth-child(2n) .num{
    font-size: 24px;
    margin-right: 5px;
}

/*动态*/
.tj-dt{
    height:108px;
    position: relative;
}
.tj-dt ul{
    height: 72px;
}
.tj-dt ul li{
    height: 24px;
    line-height: 24px;
    font-size: 12px;
}
.c-more{
    display: block;
    cursor: pointer;
    position: absolute;
    font-size: 12px;
    left:2px;
    bottom:2px;
}

/*常用功能*/
.tj-cy li{
    cursor: pointer;
    box-shadow: 1px 1px 1px rgba(213,213,213,.6);
    -webkit-transition: box-shadow .3s;
    transition: box-shadow .3s; 
}
.tj-cy li:hover{
    box-shadow: 1px 1px 4px rgba(52,52,52,.6) 
}
.tj-cy li img{
    display: block;
    width:100%;
}


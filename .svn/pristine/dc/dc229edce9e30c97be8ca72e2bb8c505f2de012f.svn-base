* {
  font-family: "Microsoft YaHei";
}
html {
  font-size: 70px;
}
body {
  background-color: #010101;
}
@media (min-width: 1400px) {
  .container {
    width: 1370px;
  }
}
.cust-menu.navbar {
  margin-bottom: 0px;
  min-height: 0.50rem;
  border: 0px;
}
.cust-menu.navbar .navbar-collapse .nav:first-child {
  margin-left: -15px;
}
.cust-menu.navbar .navbar-collapse .nav > li {
  position: initial;
  min-height: 0.54rem;
}
.cust-menu.navbar .navbar-collapse .nav > li > a {
  line-height: 0.24rem;
  font-size: 0.24rem;
  color: #ffffff;
  background-color: transparent;
  border-width: 0px;
  padding-top: 0.10rem;
  padding-bottom: 0.20rem;
  margin-bottom: -0.04rem;
  border-style: solid;
  border-color: #e55b27;
}
.cust-menu.navbar .navbar-collapse .nav > li > a:hover,
.cust-menu.navbar .navbar-collapse .nav > li > a:focus {
  background-color: transparent;
}
.cust-menu.navbar .navbar-collapse .nav > li.dropdown .caret {
  color: #e55b27;
}
.cust-menu.navbar .navbar-collapse .nav > li.dropdown .dropdown-menu.cust-sub-menu {
  right: 0px;
  background-color: #2e2e2e;
  opacity: 0.9;
  border-top: 1px solid #6c5241;
}
.cust-menu.navbar .navbar-collapse .nav > li.dropdown .dropdown-menu.cust-sub-menu > .container {
  min-height: 2.40rem;
}
.cust-menu.navbar .navbar-collapse .nav > li.dropdown .cust-dropdown-submenu {
  display: block;
  border: 0px;
  padding: 0px;
  /*right: 0px;*/
}
.cust-menu.navbar .navbar-collapse .nav > li.dropdown .cust-dropdown-submenu li {
  margin-left: 0.12rem;
  display: inline-block;
}
.cust-menu.navbar .navbar-collapse .nav > li.dropdown .cust-dropdown-submenu li a {
  display: block;
  position: relative;
  min-height: 2.40rem;
  padding-top: 0.42rem;
  padding-left: 1.42rem;
  padding-right: 0.20rem;
  min-height: 2.42rem;
}
.cust-menu.navbar .navbar-collapse .nav > li.dropdown .cust-dropdown-submenu li a:hover {
  text-decoration: none;
}
.cust-menu.navbar .navbar-collapse .nav > li.dropdown .cust-dropdown-submenu li a img {
  width: 1.30rem;
  height: 1.30rem;
  position: absolute;
  top: 0.42rem;
  left: 0px;
}
.cust-menu.navbar .navbar-collapse .nav > li.dropdown .cust-dropdown-submenu li a span {
  display: block;
  color: #FFFFFF;
}
.cust-menu.navbar .navbar-collapse .nav > li.dropdown .cust-dropdown-submenu li a .cust-fir-span {
  font-size: 0.17rem;
}
.cust-menu.navbar .navbar-collapse .nav > li.dropdown .cust-dropdown-submenu li a .cust-sec-span {
  font-size: 0.24rem;
}
.cust-menu.navbar .navbar-collapse .nav > li.dropdown.cust-dropdown2 {
  position: relative;
}
.cust-menu.navbar .navbar-collapse .nav > li.dropdown.cust-dropdown2 img {
  height: 1em;
  margin-right: 0.5em;
}
.cust-menu.navbar .navbar-collapse .nav > li.active > a,
.cust-menu.navbar .navbar-collapse .nav > li:hover > a {
  background-color: transparent;
  color: #e55b27;
  border-bottom-width: 0.04rem;
  padding-bottom: 0.16rem;
  margin-bottom: 0px;
}
.cust-menu.navbar .navbar-collapse .nav > li.active > a.dropdown-toggle,
.cust-menu.navbar .navbar-collapse .nav > li:hover > a.dropdown-toggle {
  color: #ffffff;
}
.cust-menu.navbar .navbar-collapse .nav > li.active .dropdown-menu.cust-sub-menu,
.cust-menu.navbar .navbar-collapse .nav > li:hover .dropdown-menu.cust-sub-menu {
  display: block;
}
.cust-menu.navbar .navbar-collapse .nav > li.active .dropdown-menu.cust-sub-menu2,
.cust-menu.navbar .navbar-collapse .nav > li:hover .dropdown-menu.cust-sub-menu2 {
  display: block;
}
.cust-menu.navbar .navbar-collapse .nav.navbar-right {
  font-size: 0px;
}
.cust-menu.navbar .navbar-collapse .nav.navbar-right li {
  line-height: 0.54rem;
}
.cust-menu.navbar .navbar-collapse .nav.navbar-right li .cust-icon {
  height: 0.30rem;
}
.cust-menu.navbar .navbar-collapse .nav.navbar-right li .cust-btn1 {
  margin-left: 0.06rem;
  position: relative;
  font-size: 0.24rem;
  line-height: 0.30rem;
  height: 0.30rem;
  padding-top: 0px;
  padding-bottom: 0px;
  padding-left: 0.16rem;
  padding-right: 0.16rem;
  background-color: #e55b27;
  border-radius: 0px;
  border: 0px;
}
.cust-menu.navbar .navbar-collapse .nav.navbar-right li:last-child {
  line-height: 0.54rem;
  height: 0.54rem;
  margin-right: 0.12rem;
}

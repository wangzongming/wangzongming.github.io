/*旋转动画*/
/* entire container, keeps perspective */
.flip-container {
    perspective: 1000;
    transform-style: preserve-3d;
}
.flip-container:nth-child(2n+1){
    margin-bottom: 40px;
}
/*  UPDATED! flip the pane when hovered */
.flip-container:hover .back {

    transition: .2s  cubic-bezier(0,0,0.25,1) .2s;
    -moz-transition: .2s  cubic-bezier(0,0,0.25,1) .2s; 
    -webkit-transition: .2s  cubic-bezier(0,0,0.25,1) .2s;
    -o-transition:.2s; 
     transform: scaleX(1);
    -ms-transform:scaleX(1); 	
    -moz-transform:scaleX(1); 
    -webkit-transform:scaleX(1); 
    -o-transform:scaleX(1);  
}

.flip-container:hover .front {
     transition: .2s  cubic-bezier(1,0,0.75,1);
    -moz-transition: .2s  cubic-bezier(1,0,0.75,1); 
    -webkit-transition: .2s  cubic-bezier(1,0,0.75,1);
    -o-transition:.2s;  
     transform: scaleX(0);
    -ms-transform:scaleX(0); 	
    -moz-transform:scaleX(0); 
    -webkit-transform:scaleX(0); 
    -o-transform:scaleX(0);  
}






.flip-container,
.front,
.back {
    width: 256px;
    height: 254px;
}


/* flip speed goes here */

.flipper {
    transition: 0.6s;
    -moz-transition: 0.6s; /* Firefox 4 */
    -webkit-transition: 0.6s; /* Safari 和 Chrome */
    -o-transition:0.6s; /* Opera */
    -webkit-transform-style: preserve-3d;	/* Safari 和 Chrome */
    transform-style: preserve-3d;
    position: relative;
}
/* hide back of pane during swap */

.front,
.back {

    position: absolute;
    top: 0;
    left: 0;
}


/*  UPDATED! front pane, placed above back */

.front {
    z-index: 2;

    transition: .2s  cubic-bezier(0,0,0.25,1) .2s;
    -moz-transition: .2s  cubic-bezier(0,0,0.25,1) .2s; 
    -webkit-transition: .2s  cubic-bezier(0,0,0.25,1) .2s;
    -o-transition:.2s; 
    transform: scaleX(1);
    -ms-transform:scaleX(1); 	
    -moz-transform:scaleX(1); 
    -webkit-transform:scaleX(1); 
    -o-transform:scaleX(1); 
}


/* back, initially hidden pane */

.back {
     transition: .2s  cubic-bezier(1,0,0.75,1);
    -moz-transition: .2s  cubic-bezier(1,0,0.75,1); 
    -webkit-transition: .2s  cubic-bezier(1,0,0.75,1);
    -o-transition:.2s; 
    transform: scaleX(0);
    -ms-transform:scaleX(0); 	
    -moz-transform:scaleX(0); 
    -webkit-transform:scaleX(0); 
    -o-transform:scaleX(0); 
}

